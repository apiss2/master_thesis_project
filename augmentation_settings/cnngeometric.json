{
    "output_size":256,

    "preprocess": [
        {
            "transform":"resize",
            "params":{
                "height":512,
                "width":512
            }
        }
    ],

    "after": [
        {
            "transform":"crop",
            "params":{
                "type":"centercrop",
                "height":256,
                "width":256
            }
        }
    ],

    "augmentation":[
        {
            "transform":"flip",
            "params":{
                "type":"holizontal",
                "probability":0.5
            }
        },
        {
            "transform":"affine",
            "params":{
                "scale":0.1,
                "rotate":5,
                "shift":0.05,
                "border_mode":4,
                "probability":1
            }
        },
        {
            "transform":"noise",
            "params":{
                "type":"AdditiveGaussianNoise",
                "probability":0.5
            }
        },
        {
            "transform":"distortion",
            "params":{
                "type":"grid",
                "probability":0.5,
                "border_mode":4,
                "num_steps":5
            }
        },
        {
            "transform":"brightnesscontrast",
            "params":{
                "brightness_limit":0.3,
                "contrast_limit":0.3,
                "probability":0.95
            }
        }
    ]
}